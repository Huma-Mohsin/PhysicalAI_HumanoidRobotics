{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "buildCommand": "cd humanoid_robot_book && npm install && npm run build",
  "outputDirectory": "humanoid_robot_book/build",
  "installCommand": "cd humanoid_robot_book && npm install",
  "framework": null,
  "cleanUrls": true,
  "trailingSlash": false,
  "rewrites": [
    { "source": "/docs/:path*", "destination": "/docs/:path*.html" },
    { "source": "/ur/docs/:path*", "destination": "/ur/docs/:path*.html" }
  ]
}
